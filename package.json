{
  "name": "editor-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node bin/www",
    "dev": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "prd-dev": "cross-env NODE_ENV=prd-dev pm2 start bin/pm2-pre-dev.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:local": "cross-env NODE_ENV=test-local jest --runInBand --passWithNoTests --colors --forceExit",
    "test:remote": "cross-env NODE_ENV=test-remote jest --runInBand --passWithNoTests --colors --forceExit",
    "lint": "eslint \"src/**/*.{js,ts}\""
  },
  "dependencies": {
    "ajv": "^8.6.3",
    "axios": "^0.21.4",
    "crypto": "^1.0.1",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "ejs": "~2.3.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.7",
    "mysql2": "^2.3.0",
    "redis": "^3.1.2",
    "require-all": "^3.0.0",
    "sequelize": "^6.6.5",
    "simple-git": "^2.45.1",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.3",
    "nodemon": "^1.19.1",
    "pm2": "^5.1.2",
    "prettier": "2.4.1"
  }
}
